# Agentic QE Fleet Environment Configuration

# Fleet Configuration
FLEET_ID=agentic-qe-fleet
FLEET_NAME="Autonomous Quality Engineering Fleet"
MAX_AGENTS=20
HEARTBEAT_INTERVAL=30000
TASK_TIMEOUT=600000

# Agent Configuration
TEST_EXECUTOR_COUNT=3
QUALITY_ANALYZER_COUNT=2
PERFORMANCE_MONITOR_COUNT=1

# Database Configuration
DB_TYPE=sqlite
DB_FILENAME=./data/fleet.db
# For PostgreSQL/MySQL (uncomment if needed)
# DB_HOST=localhost
# DB_PORT=5432
# DB_NAME=agentic_qe
# DB_USER=username
# DB_PASSWORD=password

# Logging Configuration
LOG_LEVEL=info
LOG_FORMAT=json
LOG_OUTPUTS=console,file

# API Configuration
API_PORT=3000
API_HOST=0.0.0.0
API_CORS=true
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=100

# Security Configuration (Generate secure values in production)
# API_KEY=your-api-key
# JWT_SECRET=your-jwt-secret
ENCRYPTION_ALGORITHM=aes-256-gcm
ENCRYPTION_KEY_LENGTH=32

# Configuration File Path (optional)
# CONFIG_FILE=./config/fleet.yaml