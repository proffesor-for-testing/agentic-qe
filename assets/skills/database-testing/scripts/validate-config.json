{
  "skillName": "database-testing",
  "skillVersion": "1.0.0",
  "requiredTools": [
    "jq"
  ],
  "optionalTools": [
    "psql",
    "mysql",
    "mongo",
    "sqlite3",
    "node",
    "ajv",
    "jsonschema",
    "python3"
  ],
  "schemaPath": "schemas/output.json",
  "requiredFields": [
    "skillName",
    "status",
    "output",
    "output.summary",
    "output.findings",
    "output.testTypes",
    "output.databaseInfo"
  ],
  "requiredNonEmptyFields": [
    "output.summary"
  ],
  "mustContainTerms": [
    "database",
    "schema",
    "table"
  ],
  "mustNotContainTerms": [
    "TODO",
    "placeholder",
    "FIXME"
  ],
  "enumValidations": {
    ".status": [
      "success",
      "partial",
      "failed",
      "skipped"
    ]
  }
}
