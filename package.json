    "cli-table3": "^0.6.5",
    "commander": "^14.0.1",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "fs-extra": "^11.1.1",
    "graphql": "^16.11.0",
    "inquirer": "^8.2.6",
    "openai": "^6.9.1",
    "ora": "^5.4.1",
    "playwright": "^1.57.0",
    "ruvector": "^0.1.24",
    "uuid": "^11.0.5",
    "winston": "^3.18.3",
    "ws": "^8.18.3",
    "yaml": "^2.8.1"
  },
  "optionalDependencies": {
    "@ruvector/node-linux-arm64-gnu": "^0.1.16",
    "@ruvector/node-linux-x64-gnu": "^0.1.16",
