{
  "name": "agentic-qe",
  "version": "1.0.0",
  "description": "AI-powered Quality Engineering framework with autonomous testing agents",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "aqe": "dist/cli/aqe.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/proffesor-for-testing/agentic-qe.git"
  },
  "bugs": {
    "url": "https://github.com/proffesor-for-testing/agentic-qe/issues"
  },
  "homepage": "https://github.com/proffesor-for-testing/agentic-qe#readme",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli/aqe.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "start": "node dist/cli/aqe.js",
    "cli": "ts-node src/cli/aqe.ts",
    "mcp": "node mcp-server.js",
    "mcp:start": "node mcp-server.js",
    "mcp:config": "node scripts/configure-mcp.js",
    "mcp:restart": "pkill -f mcp-server.js 2>/dev/null; node mcp-server.js",
    "mcp:test": "QE_MCP_SUMMARY_MODE=true node mcp-server.js",
    "init:agents": "node scripts/init-agents.js",
    "benchmark": "node benchmark/qe-benchmark.js",
    "benchmark:full": "node benchmark/qe-benchmark.js --iterations 10 --warmup 3",
    "check": "./scripts/check-prerequisites.sh",
    "postinstall": "npm run build && node scripts/init-agents.js 2>/dev/null && echo '\nâœ… Run npm run check to verify prerequisites'"
  },
  "keywords": [
    "quality-engineering",
    "testing-with-agents",
    "ai-testing",
    "agentic-orchestration",
    "agentic-qe",
    "ai-agents",
    "automation",
    "exploratory-testing"
  ],
  "author": "Dragan Spiridonov",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "date-fns": "^2.30.0",
    "fs-extra": "^11.1.1",
    "glob": "^11.0.3",
    "inquirer": "^9.2.12",
    "ora": "^5.4.1",
    "table": "^6.9.0",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "yaml": "^2.8.1",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.2",
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.8.0",
    "@types/uuid": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.2",
    "ts-node": "^10.9.1",
    "tsx": "^4.20.5",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
