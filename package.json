{
  "name": "agentic-qe",
  "version": "1.3.4",
  "description": "Agentic Quality Engineering Fleet System - AI-driven quality management platform with 34 QE skills, learning, pattern reuse, ML-based flaky detection, Multi-Model Router (70-81% cost savings), streaming progress updates, 61 MCP tools, and native TypeScript hooks",
  "main": "dist/cli/index.js",
  "types": "dist/cli/index.d.ts",
  "bin": {
    "agentic-qe": "./bin/agentic-qe",
    "aqe": "./bin/aqe",
    "aqe-mcp": "./bin/aqe-mcp"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli/index.ts",
    "start": "node dist/cli/index.js",
    "pretest": "node scripts/check-memory-before-test.js",
    "test": "ulimit -n 4096 2>/dev/null || true && node --expose-gc --max-old-space-size=2048 --no-compilation-cache node_modules/.bin/jest --maxWorkers=1 --forceExit",
    "pretest:safe": "node scripts/check-memory-before-test.js",
    "test:safe": "node --expose-gc --max-old-space-size=768 --no-compilation-cache node_modules/.bin/jest --maxWorkers=1 --runInBand --bail --detectLeaks --forceExit --logHeapUsage",
    "pretest:unit-only": "node scripts/check-memory-before-test.js",
    "test:unit-only": "node --expose-gc --max-old-space-size=512 --no-compilation-cache node_modules/.bin/jest tests/unit --maxWorkers=1 --bail --forceExit",
    "pretest:unit": "node scripts/check-memory-before-test.js",
    "test:unit": "node --expose-gc --max-old-space-size=512 --no-compilation-cache node_modules/.bin/jest tests/unit --runInBand",
    "pretest:integration-serial": "node scripts/check-memory-before-test.js",
    "test:integration-serial": "node --expose-gc --max-old-space-size=768 --no-compilation-cache node_modules/.bin/jest tests/integration --runInBand --detectLeaks --forceExit",
    "pretest:integration": "node scripts/check-memory-before-test.js",
    "test:integration": "node --expose-gc --max-old-space-size=768 --no-compilation-cache node_modules/.bin/jest tests/integration --runInBand --forceExit",
    "pretest:performance-isolated": "node scripts/check-memory-before-test.js",
    "test:performance-isolated": "node --expose-gc --max-old-space-size=1536 --no-compilation-cache node_modules/.bin/jest tests/performance --runInBand --maxWorkers=1 --detectLeaks --forceExit --logHeapUsage",
    "pretest:performance": "node scripts/check-memory-before-test.js",
    "test:performance": "node --expose-gc --max-old-space-size=1536 --no-compilation-cache node_modules/.bin/jest tests/performance --runInBand --forceExit",
    "test:watch": "node --expose-gc --max-old-space-size=512 node_modules/.bin/jest --watch --maxWorkers=1",
    "pretest:coverage": "node scripts/check-memory-before-test.js",
    "test:coverage": "node --expose-gc --max-old-space-size=3072 --no-compilation-cache node_modules/.bin/jest --coverage --maxWorkers=1",
    "pretest:coverage-safe": "node scripts/check-memory-before-test.js",
    "test:coverage-safe": "node --expose-gc --max-old-space-size=1024 --no-compilation-cache node_modules/.bin/jest --coverage --maxWorkers=1 --bail --forceExit",
    "test:memory-track": "TRACK_MEMORY=true node --expose-gc --max-old-space-size=1024 --no-compilation-cache node_modules/.bin/jest --maxWorkers=1 --runInBand",
    "test:debug": "node --expose-gc --inspect-brk --max-old-space-size=2048 node_modules/.bin/jest --runInBand --no-cache",
    "pretest:ci": "node scripts/check-memory-before-test.js",
    "test:ci": "node --expose-gc --max-old-space-size=1024 --no-compilation-cache node_modules/.bin/jest --ci --maxWorkers=1 --bail --forceExit --detectLeaks",
    "test:e2e": "node --expose-gc --max-old-space-size=768 --no-compilation-cache node_modules/.bin/jest tests/e2e --runInBand --forceExit",
    "test:agents": "node --expose-gc --max-old-space-size=512 --no-compilation-cache node_modules/.bin/jest tests/agents --runInBand",
    "test:mcp": "node --expose-gc --max-old-space-size=512 --no-compilation-cache node_modules/.bin/jest tests/mcp --runInBand",
    "test:cli": "node --expose-gc --max-old-space-size=512 --no-compilation-cache node_modules/.bin/jest tests/cli --runInBand",
    "test:utils": "node --expose-gc --max-old-space-size=768 --no-compilation-cache node_modules/.bin/jest tests/utils --runInBand",
    "test:streaming": "node --expose-gc --max-old-space-size=512 --no-compilation-cache node_modules/.bin/jest tests/streaming --runInBand",
    "test:agentdb": "node --expose-gc --max-old-space-size=1024 --no-compilation-cache node_modules/.bin/jest tests/agentdb --runInBand",
    "test:benchmark": "node --expose-gc --max-old-space-size=2048 --no-compilation-cache node_modules/.bin/jest tests/agentdb/performance-benchmark.test.ts --runInBand --forceExit",
    "pretest:integration:phase2": "node scripts/check-memory-before-test.js",
    "test:integration:phase2": "node --expose-gc --max-old-space-size=1024 --no-compilation-cache node_modules/.bin/jest tests/integration/phase2 --runInBand --forceExit",
    "pretest:integration:phase2:agents": "node scripts/check-memory-before-test.js",
    "test:integration:phase2:agents": "node --expose-gc --max-old-space-size=1024 --no-compilation-cache node_modules/.bin/jest tests/integration/phase2/phase2-agent-integration.test.ts --runInBand --forceExit",
    "pretest:integration:phase2:cli": "node scripts/check-memory-before-test.js",
    "test:integration:phase2:cli": "node --expose-gc --max-old-space-size=768 --no-compilation-cache node_modules/.bin/jest tests/integration/phase2/phase2-cli-integration.test.ts --runInBand --forceExit",
    "pretest:integration:phase2:mcp": "node scripts/check-memory-before-test.js",
    "test:integration:phase2:mcp": "node --expose-gc --max-old-space-size=768 --no-compilation-cache node_modules/.bin/jest tests/integration/phase2/phase2-mcp-integration.test.ts --runInBand --forceExit",
    "pretest:integration:phase2:e2e": "node scripts/check-memory-before-test.js",
    "test:integration:phase2:e2e": "node --expose-gc --max-old-space-size=1024 --no-compilation-cache node_modules/.bin/jest tests/integration/phase2/phase2-e2e-workflows.test.ts --runInBand --forceExit",
    "pretest:integration:phase2:perf": "node scripts/check-memory-before-test.js",
    "test:integration:phase2:perf": "node --expose-gc --max-old-space-size=1536 --no-compilation-cache node_modules/.bin/jest tests/integration/phase2/phase2-performance-benchmarks.test.ts --runInBand --forceExit",
    "pretest:integration:phase2:resources": "node scripts/check-memory-before-test.js",
    "test:integration:phase2:resources": "node --expose-gc --max-old-space-size=1536 --no-compilation-cache node_modules/.bin/jest tests/integration/phase2/phase2-resource-usage.test.ts --runInBand --detectLeaks --forceExit",
    "test:sequential": "npm run test:unit && npm run test:agents && npm run test:mcp && npm run test:cli && npm run test:utils && npm run test:integration && npm run test:performance",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "prepare": "npm run build",
    "prepublishOnly": "npm run typecheck && npm run build",
    "mcp:start": "node dist/mcp/start.js",
    "mcp:dev": "nodemon --watch src/mcp -e ts --exec \"npm run build && node dist/mcp/start.js\"",
    "docs:api": "typedoc --out docs/api src --exclude '**/*.test.ts' --excludePrivate",
    "docs:serve": "npx http-server docs/api -p 8080",
    "orchestrator": "ts-node scripts/final-go-orchestrator.ts",
    "orchestrator:test": "ts-node scripts/test-orchestrator.ts",
    "query-memory": "ts-node scripts/query-aqe-memory-single.ts",
    "verify:counts": "tsx scripts/verify-counts.ts",
    "verify:agent-skills": "tsx scripts/verify-agent-skills.ts",
    "verify:features": "tsx scripts/verify-features.ts",
    "verify:all": "npm run verify:counts && npm run verify:agent-skills && npm run verify:features",
    "update:counts": "tsx scripts/update-documentation-counts.ts"
  },
  "keywords": [
    "quality-engineering",
    "testing",
    "automation",
    "ai-agents",
    "distributed-systems",
    "swarm-intelligence",
    "typescript-hooks",
    "native-hooks",
    "lifecycle-management",
    "mcp-integration",
    "agent-coordination",
    "machine-learning",
    "reinforcement-learning",
    "pattern-matching",
    "flaky-test-detection",
    "continuous-improvement",
    "cost-optimization",
    "test-generation",
    "coverage-analysis"
  ],
  "author": "AQE Development Team",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.64.0",
    "@babel/parser": "^7.24.0",
    "@babel/traverse": "^7.24.0",
    "@faker-js/faker": "^10.0.0",
    "@modelcontextprotocol/sdk": "^1.18.2",
    "@xenova/transformers": "^2.6.0",
    "agentdb": "^1.0.0",
    "agentic-flow": "^1.7.3",
    "agentic-qe": "^1.3.3",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "better-sqlite3": "^12.4.1",
    "chalk": "^4.1.2",
    "chokidar": "^3.6.0",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.1",
    "dotenv": "^17.2.3",
    "fs-extra": "^11.1.1",
    "graphql": "^16.11.0",
    "inquirer": "^8.2.6",
    "ora": "^5.4.1",
    "uuid": "^11.0.5",
    "winston": "^3.18.3",
    "ws": "^8.14.2",
    "yaml": "^2.8.1"
  },
  "resolutions": {
    "graceful-fs": "^4.2.11",
    "stack-utils": "^2.0.6"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.0.0",
    "@jest/globals": "^30.2.0",
    "@jest/test-sequencer": "^30.2.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/fs-extra": "^11.0.4",
    "@types/graphql": "^14.2.3",
    "@types/inquirer": "^8.2.10",
    "@types/istanbul-lib-coverage": "^2.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.17",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "commander": "^14.0.1",
    "dotenv": "^17.2.3",
    "eslint": "^8.55.0",
    "eslint-plugin-security": "^3.0.1",
    "graceful-fs": "^4.2.11",
    "graphql": "^16.11.0",
    "istanbul-lib-coverage": "^3.2.2",
    "jest": "^30.2.0",
    "jest-extended": "^6.0.0",
    "nodemon": "^3.0.2",
    "rimraf": "^6.0.1",
    "stack-utils": "^2.0.6",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.1",
    "tsx": "^4.20.6",
    "typedoc": "^0.28.13",
    "typescript": "^5.9.3",
    "uuid": "^11.0.5",
    "winston": "^3.18.3",
    "yaml": "^2.8.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/proffesor-for-testing/agentic-qe.git"
  },
  "bugs": {
    "url": "https://github.com/proffesor-for-testing/agentic-qe/issues"
  },
  "homepage": "https://github.com/proffesor-for-testing/agentic-qe#readme",
  "files": [
    "dist",
    "bin",
    ".claude",
    "config",
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "CONTRIBUTING.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
