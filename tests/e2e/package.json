{
  "name": "@agentic-qe/e2e-tests-sauce-demo",
  "version": "1.0.0",
  "description": "E2E tests for Sauce Demo Shopify store - Generated by qe-queen-coordinator",
  "private": true,
  "scripts": {
    "test": "playwright test",
    "test:chromium": "playwright test --project=chromium",
    "test:firefox": "playwright test --project=firefox",
    "test:webkit": "playwright test --project=webkit",
    "test:mobile": "playwright test --project=mobile-chrome --project=mobile-safari",
    "test:critical": "playwright test --grep @critical",
    "test:security": "playwright test --grep @security",
    "test:purchase": "playwright test --grep @purchase",
    "test:cart": "playwright test --grep @cart",
    "test:auth": "playwright test --grep @auth",
    "test:search": "playwright test --grep @search",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "report": "playwright show-report",
    "codegen": "playwright codegen https://sauce-demo.myshopify.com"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "e2e",
    "playwright",
    "shopify",
    "testing",
    "qe",
    "agentic-qe"
  ],
  "author": "qe-queen-coordinator",
  "license": "MIT"
}
