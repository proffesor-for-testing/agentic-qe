      "id": 3,
      "options": {
        "legend": {
          "calcs": ["mean", "max"],
          "displayMode": "table",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "editorMode": "code",
          "expr": "sum by(agent_type, span_name) (rate(aqe_agent_execution_time_seconds_sum{service_name=\"${service}\"}[$__rate_interval])) / sum by(agent_type, span_name) (rate(aqe_agent_execution_time_seconds_count{service_name=\"${service}\"}[$__rate_interval]))",
          "legendFormat": "{{agent_type}} - {{span_name}}",
          "range": true,
          "refId": "A"
        }
      ],
      "title": "Agent Execution Timeline",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
