      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "editorMode": "code",
          "expr": "topk(10, sum by(endpoint) (rate(aqe_errors_total{service_name=\"${service}\"}[$__rate_interval])) / sum by(endpoint) (rate(aqe_requests_total{service_name=\"${service}\"}[$__rate_interval])))",
          "legendFormat": "{{endpoint}}",
          "range": true,
          "refId": "A"
        }
      ],
      "title": "Top Error Endpoints",
      "type": "bargauge"
    }
