        "principles": []
      },
      "expectedError": "Invalid version format. Expected semver"
    },
    {
      "id": "invalid-003",
      "description": "Missing name field",
      "constitution": {
        "version": "1.0.0",
        "principles": []
      },
      "expectedError": "Missing required field: name"
    },
    {
      "id": "invalid-004",
      "description": "Invalid principle structure",
      "constitution": {
        "version": "1.0.0",
        "name": "test-agent",
        "principles": [
          {
            "id": "principle-001"
          }
        ]
      },
      "expectedError": "Invalid principle: missing required fields (name, description)"
    },
    {
      "id": "invalid-005",
      "description": "Invalid priority value",
      "constitution": {
        "version": "1.0.0",
        "name": "test-agent",
        "principles": [
          {
            "id": "principle-001",
            "name": "test_principle",
            "description": "Test description",
            "priority": "invalid"
          }
        ]
      },
      "expectedError": "Invalid priority value. Expected: high, medium, low"
    },
    {
      "id": "invalid-006",
      "description": "Invalid constraint type",
      "constitution": {
        "version": "1.0.0",
        "name": "test-agent",
        "principles": [],
        "constraints": [
          {
            "id": "constraint-001",
            "type": "unknown_type",
            "description": "Test constraint"
          }
        ]
      },
      "expectedError": "Invalid constraint type: unknown_type"
    },
    {
      "id": "invalid-007",
      "description": "Constraint missing condition",
      "constitution": {
        "version": "1.0.0",
        "name": "test-agent",
        "principles": [],
        "constraints": [
          {
            "id": "constraint-001",
            "type": "coverage",
            "description": "Test constraint"
          }
        ]
      },
      "expectedError": "Constraint missing required field: condition"
    },
    {
      "id": "invalid-008",
      "description": "Duplicate principle IDs",
      "constitution": {
        "version": "1.0.0",
        "name": "test-agent",
        "principles": [
          {
            "id": "principle-001",
            "name": "test1",
            "description": "Test 1"
          },
          {
            "id": "principle-001",
            "name": "test2",
            "description": "Test 2"
          }
        ]
      },
      "expectedError": "Duplicate principle ID: principle-001"
    },
    {
      "id": "invalid-009",
      "description": "Empty name",
      "constitution": {
        "version": "1.0.0",
        "name": "",
        "principles": []
      },
      "expectedError": "Name cannot be empty"
    },
    {
      "id": "invalid-010",
      "description": "Invalid behaviors structure",
      "constitution": {
        "version": "1.0.0",
        "name": "test-agent",
        "principles": [],
